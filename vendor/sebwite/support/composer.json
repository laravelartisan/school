{
    "name": "sebwite/support",
    "description": "Laravel 5 support package",
    "keywords": ["laravel", "sebwite", "support", "repositories", "structure"],
    "license": "MIT",
    "authors": [ { "name": "Sebwite", "email": "info@sebwite.nl" } ],
    "require": {
        "php": ">=5.5.9",
        "danielstjules/stringy": "~1.8|~2.0",
        "illuminate/support": "~5.1",
        "illuminate/filesystem": "~5.1",
        "illuminate/config": "~5.1",
        "illuminate/console": "~5.1",
        "symfony/filesystem": "~2.7",
        "webmozart/path-util": "~1.0",
        "anahkiasen/underscore-php": "2.0.*",
        "gabrielelana/byte-units": "0.2.*"
    },
    "require-dev": {
        "sebwite/testbench": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Sebwite\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sebwite\\Tests\\Support\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
